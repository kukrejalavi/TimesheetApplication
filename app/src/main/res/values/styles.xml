    <!-- Base application theme. -->
    <style name="AppDialogTheme" parent="Theme.AppCompat.Light.Dialog">
        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="boxstyle" parent="android:Theme">

        <!--<item name="android:background">@drawable/custom_button</item>-->
        <item name="android:layout_width">160dp</item>
        <item name="android:layout_height">150dp</item>
        <item name="android:textAlignment" tools:targetApi="jelly_bean_mr1">center</item>
        <item name="android:textSize">25dp</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:bottomLeftRadius">7dp</item>
        <item name="android:bottomRightRadius">7dp</item>
        <item name="android:topLeftRadius">7dp</item>
        <item name="android:topRightRadius">7dp</item>

    </style>
    <string name="digits_text">abcdefghijklmnopqrstuvwxyz ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789&amp; </string>
    <style name="Widget.Button.White.BlueText.BlueText.FrameImage" parent="@android:style/Widget.Button">
        <item name="android:background">@drawable/border</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/whiteColor</item>
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
    </style>

